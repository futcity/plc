    "controllers": [
        { 
            "name": "rack",
            "type": "socket",
            "enabled": true,
            "socket": {
                "sockets": [
                    { "name": "switch1", "relay": "int-relay10-1", "button": "int-opto-1", "enabled": true },
                    { "name": "switch2", "relay": "int-relay10-2", "button": "int-opto-2", "enabled": true },
                    { "name": "switch3", "relay": "int-relay10-3", "button": "int-opto-3", "enabled": true },
                    { "name": "switch4", "relay": "int-relay10-4", "button": "int-opto-4", "enabled": true }
                ]
            }
        },
        {
            "name": "tank",
            "type": "watertank",
            "enabled": true,
            "watertank": {
                "pins": {
                    "pump": "int-relay16-1",
                    "fill": "int-relay16-2"
                },
                "levels": [
                    { "name": "low",    "pin": "int-opto-6", "enabled": true },
                    { "name": "middle", "pin": "int-opto-7", "enabled": true },
                    { "name": "high",   "pin": "int-opto-8", "enabled": true }
                ]
            }
        },
        {
            "name": "flowers",
            "type": "watering",
            "enabled": true,
            "watering": {
                "zones": [
                    { "name": "zone1", "pin": "int-relay10-1", "enabled": true },
                    { "name": "zone2", "pin": "int-relay10-2", "enabled": true },
                    { "name": "zone3", "pin": "int-relay10-3", "enabled": true },
                    { "name": "zone4", "pin": "int-relay10-4", "enabled": true }
                ]
            }
        },
        {
            "name": "table",
            "type": "security",
            "enabled": true,
            "security": {
                "pins": {
                    "key": "int-ow-1",
                    "alarm": {
                        "led": "int-alarm-1",
                        "buzzer": "int-relay5-1",
                        "relay": "int-relay10-4"
                    }
                },
                "sensors": [
                    { "name": "door",     "type": "microwave",  "pin": "int-opto-1", "alarm": true, "enabled": true },
                    { "name": "window",   "type": "pir",        "pin": "int-opto-2", "alarm": true, "enabled": true },
                    { "name": "entrance", "type": "reedswitch", "pin": "int-opto-3", "alarm": true, "enabled": true }
                ]
            }
        }
    ],

